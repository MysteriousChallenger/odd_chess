{
  "name": "Odd Chess",
  "description": "Odd Chess for ICS60 in F2019",
  "image": "heroku/python",
  "repository": "https://github.com/MysteriousChallenger/odd_chess/",
  "keywords": ["python", "django" ],
  "addons": [ "heroku-postgresql" ],
  "env": {
    "SECRET_KEY": {
      "description": "The secret key for the Django application.",
      "generator": "secret"
    }
  },
  "environments": {
    "test": {
      "scripts": {
        "test-setup": "python manage.py collectstatic --noinput",
        "test": "python manage.py test"
      }
    }
  }
}
